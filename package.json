        "popper.js": "1.*"
    },
    "devDependencies": {
        "@symfony/webpack-encore": "^1.4.0",
        "core-js": "^3.14.0",
        "file-loader": "^6.0.0",
        "filemanager-webpack-plugin": "^5.0.0",
        "glob-all": "^3.2.1",
        "hugo-bin": "^0.71.1",
